---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/tailwind.css";
import "animate.css";
const { title } = Astro.props;
---

<html lang="da">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/assets/images/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta
      name="keywords"
      content="La Glace, kager, konditorkager, lagkager, sortiment, historie, traditionel, hjemmelavet, bager, mousse, conditori, anledning, creme, bund, bestil kage, HTML, CSS, Tailwind, JavaScript"
    />
    <meta
      name="description"
      content="La Glace står klar til at bage de flotteste og mest unikke kager til dig og din særlige anledning. Vores bagere kreerer alt lige fra konditorkager, bryllupskager og lagkager, til små smagsfulde oplevelser i form af visitkager, kransekager og andre små fornøjelser. Kontakt os for at bestille en kage, eller udfyld vores bestillingsformular."
      <meta
      name="generator"
      content={Astro.generator}
    />
    <title>{Astro.props.title}</title>
  </head>
  <body>
    <header class="bg-gron500">
      <Header />
    </header>

    <main class="pt-32 md:pt-44">
      <slot />
    </main>

    <footer class="bg-gron500 p-s">
      <Footer />
    </footer>
  </body>
</html>
 ../../login.js
